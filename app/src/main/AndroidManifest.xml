<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eus.ehu.egunerokoapp.tta.egunerokoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentacion.RegistroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentacion.LoginActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.MenuActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.GoizeanActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.GozogozoaActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.ZerdiozuActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.EkintzakActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.PuntuazioaActivity" android:screenOrientation="portrait"/>
        <activity android:name=".presentacion.BagoazActivity" android:screenOrientation="portrait"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera" android:required="true" />
</manifest>